		<source ref="id40"/>
		<target ref="id41"/>
		<label kind="guard">t&gt;=1</label>
		<label kind="assignment">t=0</label>
	</transition>
	<transition>
		<source ref="id41"/>
		<target ref="id42"/>
		<label kind="guard">door[0]==1</label>
		<label kind="assignment">rule5=1,t=0</label>
	</transition>
