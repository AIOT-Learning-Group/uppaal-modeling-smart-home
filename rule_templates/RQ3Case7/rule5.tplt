	</transition>
	<transition>
		<source ref="id41"/>
		<target ref="id40"/>
		<label kind="guard">door[0]!=1</label>
		<label kind="assignment">rule5=0,t=0</label>
		<nail x="150" y="50"/>
		<nail x="0" y="50"/>
	</transition>
</template>
